{
	"name": "duoduorequest",
	"displayName": "DuoDuoRequest",
	"description": "",
	"version": "0.1.1",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [{
			"command": "duoduorequest.insertModuleRequest",
			"title": "插入请求模板"
		}, {
			"command": "duoduorequest.insertConfig",
			"title": "duoduorequest:init"
		}, {
			"command": "duoduorequest.cleanCatch",
			"title": "duoduorequest:clean catch"
		}],
		"menus": {
			"editor/context": [{
				"when": "resourceLangId == javascript",
				"command": "duoduorequest.insertModuleRequest",
				"group": "navigation"
			}]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.6.4",
		"@types/vscode": "^1.48.0",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"mkdirp": "^1.0.4",
		"vscode-helpers": "^7.0.2"
	}
}